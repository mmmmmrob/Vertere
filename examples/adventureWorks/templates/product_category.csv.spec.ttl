# 1 - ProductCategoryID
# 2 - ParentProductCategoryID
# 3 - Name
# 4 - rowguid
# 5 - ModifiedDate 

<#> a :Spec ; 
	:format :TSV ;
	:header_rows 0 ;
	:resource <#category> ;
	:base_uri "http://data.kasabi.com/dataset/adventure_works/product_categories/" ;
.

<#category> a :Resource ;
	:identity [ 
		:source_column 1 ; 
	] ;
	:type owl:Class ;
	:attribute
		[ :property rdfs:label; :source_column 3 ]
	;
	:relationship
		[ 
			:property rdfs:subClassOf; 
			:identity [ 
				:source_column 2 ; 
			] ;
		]
	;
.
