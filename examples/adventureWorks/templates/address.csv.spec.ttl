# Address
# 1 - Address ID
# 2 - AddressLine1
# 3 - AddressLine2 
# 4 - City
# 5 - StateProvince
# 6 - CountryRegion
# 7 - PostalCode
# 8 - row GUID
# 9 - modified date

<#> a :Spec ; 
	:format :TSV ;
	:header_rows 0 ;
	:resource <#address> ;
	:base_uri "http://data.kasabi.com/dataset/adventure_works/addresses/" ;
.

<#address> a :Resource ;
	:identity [ 
		:source_column 1 ; 
	] ;
	:type vcard:Address ;
	:attribute
		[ :property vcard:street-address; :source_column 2 ] ,
		[ :property vcard:extended-address; :source_column 3 ] ,
		[ :property vcard:locality; :source_column 4 ] ,
		[ :property vcard:region; :source_column 5 ] ,
		[ :property vcard:country-name; :source_column 6 ] ,
		[ :property vcard:postal-code; :source_column 7 ] 
	;
.

